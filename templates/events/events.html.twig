{% extends 'base.html.twig' %}
{% block body %}
  <div class="container">
    {% for event in eventArr %}
      <div class="card" style="width: 18rem;">
        <img class="{{ event.image }}" src="..." alt="Card image cap" />
        <div class="card-body">
          <h5 class="card-title">{{ event.ename }}</h5>
          <p class="card-text">{{ event.date|date('Y-m-d') }}</p>
          <p class="card-text">{{ event.time|date('H:i:s') }}</p>
          <p class="card-text">{{ event.url }}</p>
          <a href="#" class="btn btn-primary">Go somewhere</a>
        </div>
      </div>
    {% endfor %}
  </div>
{% endblock %}
